<?xml version="1.0"?>
<sdf version="1.7">
  <model name="cupa">
    <link name="base_link">

      <!-- Rough inertial properties -->
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>1e-4</ixx>
          <iyy>1e-4</iyy>
          <izz>1e-4</izz>
        </inertia>
      </inertial>

      <!-- Visual mesh -->
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>google_16k/textured.obj</uri>
            <scale>0.8 0.8 0.8</scale>
          </mesh>
        </geometry>
      </visual>

      <!-- Collision (same mesh for simplicity) -->
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>google_16k/textured.obj</uri>
            <scale>0.8 0.8 0.8</scale>
          </mesh>
        </geometry>
      </collision>

    </link>
  </model>
</sdf>
